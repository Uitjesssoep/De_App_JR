        app:layout_constraintTop_toBottomOf="@+id/tvAccountVisitCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="99dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvDownvoteCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Downvotes: 0"
        app:layout_constraintBottom_toBottomOf="@+id/viewpager_tablayout_MyAccount"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/tvUpvoteCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="118dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

