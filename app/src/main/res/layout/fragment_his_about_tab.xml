<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HisAboutTab">

    <TextView
        android:id="@+id/tvAccountNumbersHisAbout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Account numbers"
        android:textColor="@color/colorAccent"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04000002"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="113dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvHisAccountVisitCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Account visits: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvAccountNumbersHisAbout"
        app:layout_constraintTop_toBottomOf="@+id/tvAccountNumbersHisAbout"
        app:layout_constraintVertical_bias="0.008"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="104dp" />

    <TextView
        android:id="@+id/tvHisFollowersCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Followers: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toEndOf="@+id/tvHisAccountVisitCount"
        app:layout_constraintTop_toBottomOf="@+id/tvAccountNumbersHisAbout"
        app:layout_constraintVertical_bias="0.008"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="77dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvHisPostCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Posts: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvHisFollowersCount"
        app:layout_constraintTop_toBottomOf="@+id/tvHisFollowersCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="51dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvHisCommentCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comments: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvHisFollowersCount"
        app:layout_constraintTop_toBottomOf="@+id/tvHisPostCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="85dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvHisUpvoteCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Upvotes: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvAccountNumbersHisAbout"
        app:layout_constraintTop_toBottomOf="@+id/tvHisAccountVisitCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="67dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/tvHisDownvoteCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Downvotes: 0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tvAccountNumbersHisAbout"
        app:layout_constraintTop_toBottomOf="@+id/tvHisUpvoteCount"
        app:layout_constraintVertical_bias="0.005"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="86dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>